<h1>Accounts</h1>
<table>
  <tr>
    <th>Owner</th>
    <th>Debit Events</th>
    <th>Credit Events</th>
    <th>Balance</th>
  </tr>
<%= for account <- @accounts do %>

  <tr>
    <td><%= account.name %></td>
    <td><%= account.debit_events %></td>
    <td><%= account.credit_events %></td>
    <td><%= account.balance %></td>
  </tr>
<% end %>
</table>
